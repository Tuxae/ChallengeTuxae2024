	<link name="roue_gauche">
		<visual>
			<origin xyz="0 0 0" rpy="0 0 0"/>
			<geometry>
				<cylinder radius="0.05" length="0.02"/>
			</geometry>
	     	<material name="Grey1">
				<color rgba="0.9 0.9 0.9 1.0"/>
			</material>
	   </visual>
		<collision>
			<origin xyz="0 0 0" rpy="0 0 0"/>
			<geometry>
				<cylinder radius="0.05" length="0.02"/>
			</geometry>
    	</collision>
		<inertial>
